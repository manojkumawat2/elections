<div class="election_container mt-5">
    <h2 class="text-center fw-bold">Active Elections</h2>
    <hr>
    <% for(let index = 0; index < elections.length; index++) { %>
    <div class="card p-2 ms-auto me-auto mt-3 border" style="max-width: 654px;">
        <h5><%= elections[index]['title'] %></h5>
        <a href="<%= baseURL + 'voter/vote/' + elections[index]['id'] + '/' + elections[index]['constituency_id'] %>" class="mt-3 btn btn-outline-primary btn-rounded" data-mdb-ripple-color="dark">Vote Now</a>
    </div>
    <% } %>
</div>