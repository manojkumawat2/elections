<div class="container card mt-5">
    <h4>Create A New Election</h4>
    <hr>
    <form id="add_new_election" action="" method="post">
        <div class="mb-3">
            <label for="title" class="form-label">Title</label>
            <input type="text" class="form-control" id="title" aria-describedby="Title">
        </div>
        <div class="mb-3">
            <label for="title" class="form-label">Select Constituency</label>
            <select class="form-select select_constituency" aria-label="Default select example" searchable="Search here..">
                <option selected>Select Constituency</option>
                <option value="1">One</option>
                <option value="2">Two</option>
                <option value="3">Three</option>
            </select>
        </div>
        <div class="mb-3">
            <label for="title" class="form-label">Election Description</label>
            <textarea id="summernote" name="description"></textarea>
        </div>
        <button type="button" class="mb-3 btn btn-primary btn-rounded"><i class="fas fa-plus"></i> Create Election</button>
    </form>
</div>